<hibernate-mapping>
    <class name = "models.Item" table = "items">
        <id name = "id" column="id">
            <generator class="identity"/>
        </id>
        <property name = "desc" column="decs"/>
        <property name = "done" column="done"/>
        <property name = "created" column="create_date"/>
    </class>
</hibernate-mapping>